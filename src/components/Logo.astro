---
import { SITE } from 'astrowind:config';
import logoLight from '~/assets/images/logo-antares-light.png';
import logoDark from '~/assets/images/logo-antares-dark.png';
---

<picture class="block h-12 md:h-14">
  <source srcset={logoDark.src} media="(prefers-color-scheme: dark)" />
  <img src={logoLight.src} alt={SITE?.name} class="h-full w-auto object-contain" loading="eager" fetchpriority="high" />
</picture>
